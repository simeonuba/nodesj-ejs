<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="Ofmax.co.uk>
    <meta name="keywords" content="Logistics,Shipping and delivery" />
    <meta name="author" content="Ofmax" />
    <!-- The above 6 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <!-- Title -->
    <title>Ofmax</title>

    <!-- Styles -->
    <link
      href="https://fonts.googleapis.com/css?family=Poppins:400,500,700,800&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <link
      href="/static/assets/plugins/bootstrap/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      href="/static/assets/plugins/font-awesome/css/all.min.css"
      rel="stylesheet"
    />
    <link
      href="/static/assets/plugins/perfectscroll/perfect-scrollbar.css"
      rel="stylesheet"
    />
    <link href="/static/assets/plugins/pace/pace.css" rel="stylesheet" />

    <!-- Theme Styles -->
    <link href="/static/assets/css/main.min.css" rel="stylesheet" />
    <link href="/static/assets/css/custom.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="pace-done no-loader">
    <div class="page-container">
      <div class="page-sidebar ps">
        <a class="logo" href="index.html">Ofmax.co.uk</a>
        <ul class="list-unstyled accordion-menu ps">
          <li>
            <a href="admin.html"
              ><i data-feather="activity"></i>Dashboard<i
                class="fas fa-chevron-right dropdown-icon"
              ></i
            ></a>
          </li>
          <li>
            <a href="allusers.html"
              ><i data-feather="aperture"></i>Users<i
                class="fas fa-chevron-right dropdown-icon"
              ></i
            ></a>
          </li>
          <li>
            <a href="admin-orders.html"
              ><i data-feather="aperture"></i>Orders<i
                class="fas fa-chevron-right dropdown-icon"
              ></i
            ></a>
          </li>
        </ul>
        <a href="#" id="sidebar-collapsed-toggle"
          ><i data-feather="arrow-right"></i
        ></a>
      </div>
      <div class="page-content">
        <div class="page-header">
          <nav class="navbar navbar-expand-lg d-flex justify-content-between">
            <div class="header-title flex-fill">
              <a href="index.html#" id="sidebar-toggle"
                ><i data-feather="arrow-left"></i
              ></a>
              <h5>Dashboard</h5>
            </div>
            <div class="header-sarch">
              <a href="" class="btn btn-secondary">Logout</a>
            </div>
          </nav>
        </div>

        <div class="main-wrapper">
          <div class="row">
            <div class="col">
              <div class="card">
                <div class="card-body">
                  <h5 class="card-title">All Users</h5>

                  <table id="zero-conf" class="table table-bordered display" style="width: 100%">
                    <thead>
                      <tr>
                    
                        <th>Full Name</th>
                        <th>Email</th>
                        <th>Phone</th>
                        <th>Address</th>
                        <th>State</th>
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0; i < userData.length; i++) { %>
                        <td><%= userData[i].firstname %> <%= userData[i].lastname %></td>
                        <td><%= userData[i].email %></td>
                        <td><%= userData[i].phone %></td>
                        <td><%= userData[i].address %></td>
                        <td><%= userData[i].state %></td>
                        <td><a href="user/ban/<%= userData[i].id %>" class="btn btn-danger">ban</a></td>
                        
                            </tr>
                         <% } %>
                     
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>



    <!-- Javascripts -->
    <script src="static/assets/plugins/jquery/jquery-3.4.1.min.js"></script>
    <script src="https://unpkg.com/@popperjs/core@2"></script>
    <script src="static/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="https://unpkg.com/feather-icons"></script>
    <script src="static/assets/plugins/perfectscroll/perfect-scrollbar.min.js"></script>
    <script src="static/assets/plugins/pace/pace.min.js"></script>
    <script src="static/assets/js/main.min.js"></script>
  </body>
</html>
